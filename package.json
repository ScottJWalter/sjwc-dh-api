{
  "name": "sjwc-dh-api",
  "description": "DreamHost API Command Line Interface",
  "version": "0.0.1",
  "author": "Scott Walter @ScottJWalter",
  "bin": {
    "dh-api": "./bin/run"
  },
  "bugs": "https://github.com/ScottJWalter/sjwc-dh-api/issues",
  "dependencies": {
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-autocomplete": "^0.3.0",
    "@oclif/plugin-commands": "^1.3.0",
    "@oclif/plugin-help": "^3.2.2",
    "@oclif/plugin-not-found": "^1.2.4",
    "@oclif/plugin-plugins": "^1.9.5",
    "@oclif/plugin-update": "^1.3.10",
    "@oclif/plugin-warn-if-update-available": "^1.7.0",
    "@oclif/plugin-which": "^1.0.3",
    "ansi-colors": "^4.1.1",
    "cli-table": "^0.3.4",
    "conf-cli": "^0.1.9",
    "enquirer": "^2.3.6",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.51",
    "chai": "^4.3.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.8"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ScottJWalter/sjwc-dh-api",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "dh-api",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "account": {
        "description": "Account API"
      },
      "dns": {
        "description": "DNS API"
      },
      "domain": {
        "description": "Domain API"
      },
      "jabber": {
        "description": "Jabber API"
      },
      "list": {
        "description": "Announcement List API"
      },
      "mail": {
        "description": "Mail API"
      },
      "meta": {
        "description": "API metacommands"
      },
      "mysql": {
        "description": "MySQL API"
      },
      "ps": {
        "description": "Private Server API"
      },
      "rewards": {
        "description": "Rewards API"
      },
      "services": {
        "description": "Service Control API"
      },
      "user": {
        "description": "User API"
      }
    }
  },
  "repository": "ScottJWalter/sjwc-dh-api",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "gh-pages": "oclif-dev readme && cp README.md docs/index.md"
  },
  "types": "lib/index.d.ts"
}
